# cook your dish here
for  i in range(int(input())):
    n,f=int(input()),1
    s=input().split()
    if s[-1]=='cookie': print('NO')
    else:
     for i in range(len(s)):
        if s[i]=='cookie':
            if s[i+1]!='milk':  
                f=0
                break
     if f==0: print('NO')
     else: print('YES')
